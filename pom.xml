    <modules>
        <module>00-example-full</module>
        <module>01-simple-mapping</module>
        <module>02-parent-child</module>
        <module>03-cascading-persistence</module>
        <module>04-embedded-object</module>
        <module>05-inheritance</module>
        <module>06.1-queries-hql</module>
        <module>06.2-queries-criteria</module>
        <module>06.3-queries-metamodel</module>
